{
  "name": "vscode-dasm",
  "displayName": "Dasm macro assembler for VSCode",
  "description": "Develop and run Atari games from inside Visual Studio Code",
  "version": "0.0.1",
  "publisher": "zehfernando",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-dasm.openToTheSide"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "languages": [
      {
        "id": "dasm",
        "aliases": ["Dasm","Assembly", "6502"],
        "extensions": [".dasm", ".asm", ".a", ".h"],
        "configuration": "./syntaxes/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dasm",
        "scopeName": "source.asm.dasm",
        "path": "./syntaxes/dasm.json"
      }
    ],
    "commands": [
      {
        "command": "vscode-dasm.openToTheSide",
        "title": "Open Atari emulator to the side"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "bin": {
    "tslint": "./node_modules/tslint/bin/tslint"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^7.0.0",
    "mocha": "^3.2.0",
    "tslint": "^4.2.0",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "dasm": "^3.1.1",
    "fs": "0.0.1-security",
    "path": "^0.12.7"
  }
}